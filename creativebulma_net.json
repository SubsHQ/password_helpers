{
  "register": 
    {
      "url": ["https://creativebulma.net/register"],
      "container": ".container .section .column.is-half",
      "username": "#username",
      "email": "#email",
      "password": "#password",
      "passwordRepeat": "#password_confirmation",
      "passwordRequirements": {
        "min": 8,
        "upper": 1,
        "lower": 1,
        "number": 1,
        "special": 1
      }
    },
  "login":
    {
      "url": ["https://creativebulma.net/login"],
      "container": ".container .section .column.is-half",
      "email": "#email",
      "password": "#password"
    }
}