{
    "register": {
        "urlRegExp": "https:\/\/(?:register.|signup.)live.com\/?.*",
        "container": ".middle .inner",
        "email": "#MemberName",
        "password": "#PasswordInput",
        "first": "#FirstName",
        "last": "#LastName",
        "passwordRequirements": {
            "min": 8,
            "upper": 1,
            "lower": 1,
            "number": 1,
            "special": 1
        }
    },
    "login": {
        "urlRegExp": "https:\/\/login.live.com\/?.*",
        "container": ".middle .inner",
        "email": "#i0116",
        "password": "#i0118"
    }
}