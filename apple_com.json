{
  "register": {
    "urlRegExp": "https:\/\/appleid.apple.com\/?.*",
    "container": ".create-form",
    "email": ".email-field, input[type='email']",
    "password": "#password",
    "passwordRepeat": "input.confirm-field"
  },
  "login": {
    "urlRegExp": "https:\/\/(?:secure1.)?(?:appstoreconnect.|idmsa.|store.|appleid.)?apple.com\/?.*",
    "container": "#sign_in_form, #signin",
    "email": "#account_name_text_field",
    "password": "#password_text_field"
  }
}