{
  "register": {
    "url": [
      "https://login.yahoo.com/account/create"
    ],
    "container": ".login-box-container .login-box",
    "first": "#usernamereg-firstName",
    "last": "#usernamereg-lastName",
    "username": "#usernamereg-yid",
    "password": "#usernamereg-password",
    "email": "#usernamereg-email",
    "passwordRequirements": {
      "min": 8,
      "upper": 1,
      "lower": 1,
      "number": 1,
      "special": 1
    }
  },
  "login": {
    "url": [
      "https://login.yahoo.com/"
    ],
    "container": ".login-content .login-box",
    "username": "#login-username",
    "password": "[type='password']"
  }
}