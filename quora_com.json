{
    "register": {
        "url": [
            "https://www.quora.com/"
        ],
        "container": ".content_inner .signup_login .signup",
        "first": ".signup input[name='first_name']",
        "last": ".signup input[name='last_name']",
        "password": ".signup input[name='password']",
        "email": ".signup input[name='email']",
        "passwordRequirements": {
            "min": 8,
            "upper": 1,
            "lower": 1,
            "number": 1,
            "special": 1
        }
    },
    "login": {
        "url": [
            "https://www.quora.com/"
        ],
        "container": ".content_inner .signup_login .login",
        "email": ".login input[name='email']",
        "password": ".login input[name='password']"
    }
}